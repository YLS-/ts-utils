{
	"name": "@yohs/ts-core",
	"version": "0.2.4",
	"license": "MIT",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./array": {
			"types": "./dist/array/methods.d.ts",
			"import": "./dist/array/methods.js",
			"require": "./dist/array/methods.cjs"
		},
		"./array/register": {
			"types": "./dist/array/register.d.ts",
			"import": "./dist/array/register.js",
			"require": "./dist/array/register.cjs"
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"sideEffects": [
		"./dist/array/register.js"
	],
	"engines": {
		"node": ">=18"
	},
	"dependencies": {
		"mime-types": "^3.0.1"
	},
	"devDependencies": {
		"@types/mime-types": "^3.0.1",
		"rimraf": "^6.0.1"
	},
	"scripts": {
		"build": "tsup",
		"clean": "rimraf dist",
		"test": "vitest run",
		"lint": "eslint 'src/**/*.{ts,tsx}'",
		"prepublishOnly": "pnpm run build && pnpm run test"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/YLS-/ts-utils.git",
		"directory": "packages/ts-core"
	},
	"homepage": "https://github.com/YLS-/ts-utils/tree/main/packages/ts-core",
	"bugs": {
		"url": "https://github.com/YLS-/ts-utils/issues"
	}
}
