{
	"name": "@yohs/firebase-utils",
	"version": "0.0.1",
	"description": "Firebase Admin SDK utilities",
	"license": "MIT",
	"type": "module",
	"engines": {
		"node": ">=20"
	},
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
